{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "build",
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
    // Sets the cache header for ** to cache for 5 minutes
  "source" : "**",
  "headers" : [ {
    "key" : "Cache-Control",
    "value" : "max-age=300"
  } ]
}

